{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "be:dev": "nodemon -w src/server -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "fe:dev": "vite",
    "dev": "concurrently \"npm run be:dev\" \"npm run fe:dev\"",
    "build": "vite build",
    "seed": "tsx src/scripts/seed.ts",
    "test": "tsx src/scripts/test.ts",
    "migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@dsnp/parquetjs": "^1.6.0",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-select": "^2.0.0",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lucide-react": "^0.394.0",
    "prisma": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.3.0",
    "typescript": "^5.3.2",
    "vite-express": "*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.3",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.2.16",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.2"
  }
